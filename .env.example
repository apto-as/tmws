# TMWS Environment Configuration
# Copy this file to .env and customize for your environment

# Agent Configuration
TMWS_AGENT_ID=athena-conductor
TMWS_AGENT_NAMESPACE=trinitas
TMWS_ALLOW_DEFAULT_AGENT=true

# Database Configuration (Required)
# PostgreSQL with pgvector extension is required
TMWS_DATABASE_URL=postgresql://tmws_user:tmws_password@localhost:5432/tmws

# Security
# If not set, a random key will be generated in MCP mode
# TMWS_SECRET_KEY=your-secret-key-here

# Optional: Logging
TMWS_LOG_LEVEL=INFO

# Optional: MCP Mode Detection
# Set to true to force MCP mode
MCP_MODE=true