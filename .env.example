# TMWS Environment Configuration
# Copy this file to .env and customize for your environment

# Agent Configuration
TMWS_AGENT_ID=athena-conductor
TMWS_AGENT_NAMESPACE=trinitas
TMWS_ALLOW_DEFAULT_AGENT=true

# Database Configuration (Optional for MCP mode)
# If not set, SQLite will be used in MCP mode
# TMWS_DATABASE_URL=postgresql://user:password@localhost/tmws_db

# Security
# If not set, a random key will be generated in MCP mode
# TMWS_SECRET_KEY=your-secret-key-here

# Optional: Logging
TMWS_LOG_LEVEL=INFO

# Optional: MCP Mode Detection
# Set to true to force MCP mode
MCP_MODE=true