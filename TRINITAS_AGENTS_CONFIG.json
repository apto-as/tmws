  "shared_configuration": {
    "description": "全Trinitasエージェント共通設定",
    "database": {
      "host": "localhost",
      "port": 5432,
      "database": "tmws_trinitas",
      "user": "tmws_user",
      "sslmode": "prefer",
      "pool_size": 10,
      "max_overflow": 20
    },
    "vector_search": {
      "TMWS_EMBEDDING_MODEL": "all-MiniLM-L6-v2",
      "TMWS_VECTOR_DIMENSION": "384",
      "TMWS_MAX_EMBEDDING_BATCH_SIZE": "32"
    },
    "api_settings": {
      "TMWS_API_HOST": "127.0.0.1",
      "TMWS_API_PORT": "8000",
      "TMWS_CORS_ORIGINS": "[\"http://localhost:3000\", \"http://localhost:8080\"]"
    },
    "rate_limiting": {
      "TMWS_RATE_LIMIT_ENABLED": "true",
      "TMWS_RATE_LIMIT_REQUESTS": "1000",
      "TMWS_RATE_LIMIT_PERIOD": "60"
    },
    "memory_access_rules": {
      "athena": {
        "access_level": "system",
        "can_access": ["all"],
        "share_with": ["artemis", "hestia", "eris", "hera", "muses"]
      },
      "artemis": {
        "access_level": "team",
        "can_access": ["team", "public", "system"],
        "share_with": ["athena", "hestia", "eris"]
      },
      "hestia": {
        "access_level": "system",
        "can_access": ["all"],
        "share_with": ["athena"],
        "special_permissions": ["audit_all", "security_override"]
      },
      "eris": {
        "access_level": "team",
        "can_access": ["team", "public", "system"],
        "share_with": ["athena", "artemis", "hera"]
      },
      "hera": {
        "access_level": "team",
        "can_access": ["team", "public", "system"],
        "share_with": ["athena", "eris", "muses"]
      },
      "muses": {
        "access_level": "public",
        "can_access": ["public", "system"],
        "share_with": ["all"],
        "special_permissions": ["read_all_documentation"]
      }
    }
  },
  
  "deployment_notes": {
    "description": "デプロイメント時の注意事項",
    "requirements": [
      "PostgreSQL 14+ with pgvector extension",
      "Python 3.10+",
      "Redis (optional, for caching)",
      "SSL certificates for production"
    ],
    "setup_commands": [
      "createdb tmws_trinitas",
      "psql -d tmws_trinitas -c 'CREATE EXTENSION IF NOT EXISTS vector;'",
      "alembic upgrade head",
      "python scripts/setup_security.py"
    ],
    "security_notes": [
      "各エージェントごとに異なるSECRET_KEYを使用",
      "本番環境では必ずSSL/TLSを有効化",
      "定期的なキーローテーション実施",
      "監査ログの定期バックアップ"
    ]
  }
}